{
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "atomKeymap.promptV3Features": true,
    "backgroundCover.autoStatus": true,
    "backgroundCover.imagePath": "c:\\Users\\denjj\\Documents\\GitHub\\pixivwall\\images\\2020年5月1日\\81176696_p4.jpg",
    "backgroundCover.opacity": 0.6,
    "backgroundCover.randomImageFolder": "c:\\Users\\denjj\\Documents\\GitHub\\pixivwall\\images\\2020年5月1日",
    "code-runner.saveFileBeforeRun": true,
    "codespaces.accountProvider": "Microsoft",
    "crystal-lang.bashOnWindows": true,
    "crystal-lang.completion": true,
    "crystal-lang.hover": true,
    "crystal-lang.implementations": true,
    "crystal-lang.problems": "build",
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "editor.fontFamily": "Ricty Diminished with Fira Code",
    "editor.fontLigatures": true,
    "editor.formatOnPaste": true,
    "editor.lineHeight": 13,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.renderControlCharacters": true,
    "editor.renderWhitespace": "boundary",
    "editor.tabSize": 2,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "vue"
    ],
    "files.exclude": {
        "**/*.meta": true
    },
    "files.associations": {
        "*.sage": "python"
    },
    "git.autofetch": true,
    "git.confirmSync": false,
    "liveshare.presence": true,
    "problems.showCurrentInStatus": true,
    "python.insidersChannel": "off",
    "sync.gist": "",
    "task.autoDetect": "off",
    "vim.useCtrlKeys": false,
    "vsicons.dontShowNewVersionMessage": true,
    "workbench.editor.limit.enabled": true,
    "workbench.editor.limit.perEditorGroup": true,
    "workbench.editor.limit.value": 6,
    "workbench.editor.tabSizing": "shrink",
    "workbench.iconTheme": "vscode-icons",
    "zentabs.maximumOpenedTabs": 12,
    "git.enableSmartCommit": true,
    "csharp.suppressDotnetInstallWarning": true,
    "csharp.suppressDotnetRestoreNotification": true,
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "[html]": {
        "editor.defaultFormatter": "numso.prettier-standard-vscode"
    },
    "remote.SSH.remotePlatform": {
        "tsg": "linux",
        "rainbow": "linux",
        "sail": "linux"
    },
    "mssql.connections": [
        {
            "server": "{{put-server-name-here}}",
            "database": "{{put-database-name-here}}",
            "user": "{{put-username-here}}",
            "password": ""
        }
    ],
    "C_Cpp.updateChannel": "Insiders",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "terminal.integrated.scrollback": 100000,
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "*": true,
        "yaml": false,
        "plaintext": true,
        "markdown": false
    },
    "python.showStartPage": false,
    "terminal.integrated.tabs.enabled": false,
    "go.toolsManagement.autoUpdate": true,
    "security.workspace.trust.untrustedFiles": "open",
    "git.defaultCloneDirectory": "C:\\Users\\denjj\\Documents\\GitHub",
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "workbench.colorTheme": "Base16 Dark Colors",
    "remote.SSH.defaultExtensions": [
        "gitpod.gitpod-remote-ssh"
    ]
}
